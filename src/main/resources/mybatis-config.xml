<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE configuration PUBLIC
"-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<!-- 마이바티스 설정과 관련된 태그 -->
	<settings>
		<!-- 만약에 Null 값이 들어온다면 DB에서도  Null 이라고 받아라! -->
		<setting name="jdbcTypeForNull" value="NULL"/>
	</settings>
	<typeAliases>
<!-- 		 sql에서 사용할 때 직접 내용을 적지 않고 별칭으로도 접근이 가능하다. -->
<!-- 		회원용 -->
	
		<typeAlias type="org.covid19.ourapp.VO.MemberVO" alias="Member"/>
		<typeAlias type="org.covid19.ourapp.VO.BoardVO" alias="BoardVO"/>
</typeAliases>

	</configuration>